{
  "name": "help",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "ci": "yarn lint && yarn prettier:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,md}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,md}\"",
    "api": "docusaurus gen-api-docs all",
    "api.mocks": "mock-to-openapi api/mocks",
    "api.clean": "docusaurus clean-api-docs all",
    "api.regenerate": "yarn api.clean && yarn api"
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.1",
    "@docusaurus/module-type-aliases": "^3.1.1",
    "@docusaurus/plugin-content-docs": "^3.1.1",
    "@docusaurus/plugin-ideal-image": "^3.1.1",
    "@docusaurus/preset-classic": "^3.1.1",
    "@mdx-js/react": "^3.0.1",
    "classnames": "^2.5.1",
    "docusaurus-lunr-search": "^3.3.2",
    "docusaurus-plugin-openapi-docs": "3.0.0-beta.5",
    "docusaurus-theme-openapi-docs": "3.0.0-beta.5",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/types": "^3.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mock-to-openapi": "^1.0.9",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1"
  },
  "engines": {
    "node": ">=18.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
