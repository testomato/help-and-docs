{
	"name": "help",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "docusaurus start",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"ci": "yarn lint && yarn prettier:diff",
		"lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
		"prettier": "prettier --config .prettierrc --write \"**/*.{js,md}\"",
		"prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,md}\"",
		"api": "docusaurus gen-api-docs all",
		"api.mocks": "mock-to-openapi api/mocks",
		"api.clean": "docusaurus clean-api-docs all",
		"api.regenerate": "yarn api.clean && yarn api"
	},
	"dependencies": {
		"@docusaurus/core": "^2.3.1",
		"@docusaurus/preset-classic": "^2.3.1",
		"classnames": "^2.3.2",
		"docusaurus-lunr-search": "^2.3.2",
		"docusaurus-plugin-openapi-docs": "2.0.0-beta.2",
		"docusaurus-theme-openapi-docs": "2.0.0-beta.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"eslint": "^8.36.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-header": "^3.1.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"mock-to-openapi": "^1.0.9",
		"prettier": "^2.8.6",
		"stylelint": "^15.3.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
